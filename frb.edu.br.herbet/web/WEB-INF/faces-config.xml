<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>vaiParaListagem</from-outcome>
            <to-view-id>/PaisListagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaCidade</from-outcome>
            <to-view-id>/CidadeListagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaVendasListagem</from-outcome>
            <to-view-id>/VendasListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/PaisListagem.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaIncluir</from-outcome>
            <to-view-id>/PaisIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEditar</from-outcome>
            <to-view-id>/PaisEditar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaDetalhe</from-outcome>
            <to-view-id>/PaisDetalhe.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/PaisListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>paisController</managed-bean-name>
        <managed-bean-class>frb.edu.br.controladores.PaisController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/PaisIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/PaisListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/PaisEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/PaisListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/PaisDetalhe.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/PaisListagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEditar</from-outcome>
            <to-view-id>/PaisEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/CidadeListagem.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaCidadeIncluir</from-outcome>
            <to-view-id>/CidadeIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaCidadeDetalhe</from-outcome>
            <to-view-id>/CidadeDetalhe.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaCidadeEditar</from-outcome>
            <to-view-id>/CidadeEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>cidadeController</managed-bean-name>
        <managed-bean-class>frb.edu.br.controladores.CidadeController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/CidadeIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaCidadeListagem</from-outcome>
            <to-view-id>/CidadeListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/CidadeDetalhe.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaCidadeListagem</from-outcome>
            <to-view-id>/CidadeListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/CidadeEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaCidadeListagem</from-outcome>
            <to-view-id>/CidadeListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>VendasController</managed-bean-name>
        <managed-bean-class>frb.edu.br.controladores.VendasController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/VendasListagem.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaVendasIncluir</from-outcome>
            <to-view-id>/VendasIncluir.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/VendasIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaVendasListagem</from-outcome>
            <to-view-id>/VendasListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
